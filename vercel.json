{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/build/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/build/(.*)",
        "dest": "/public/build/$1"
      }
    ]
  }
  